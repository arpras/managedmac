# A sample Gemfile
source 'https://rubygems.org'

group :development, :test do
  gem 'pry'
  gem 'puppet-lint'
  gem 'puppetlabs_spec_helper'
  gem 'hiera-puppet-helper', :git => 'git://github.com/mmz-srf/hiera-puppet-helper.git'
  gem 'CFPropertyList'
  gem 'sqlite3'
end

group :production do
  gem 'CFPropertyList'
  gem 'sqlite3'
end
